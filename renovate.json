{
  "endpoint":"https://api.github.com",
  "platform":"github",
  "renovateFork":true,
  "includeForks":true,
  "labels":[
     "renovate",
     "dependencies",
     "automated"
  ],
  "onboarding":true,
  "onboardingConfig":{
     "extends":[
        "config:base",
        "docker:enableMajor"
     ]
  },
  "docker":{
      "packageRules": [
        {
          "packageNames": [
            "birdhouse/pavics-jupyter-base"
          ],
          "groupName": "base image update"
       }
      ]
  },
  "extends":[
     "config:base",
     "docker:enableMajor"
  ],
  "prHourlyLimit":0,
  "prConcurrentLimit": 20,
  "logFile":"renovate.log",
  "logFileLevel":"debug",
  "logLevel":"info"
}
